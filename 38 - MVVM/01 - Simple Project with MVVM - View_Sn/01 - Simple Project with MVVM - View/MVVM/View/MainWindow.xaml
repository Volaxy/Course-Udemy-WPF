<Window x:Class="_01___Simple_Project_with_MVVM___View.MVVM.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_01___Simple_Project_with_MVVM___View.MVVM.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="214" Width="342">
    <Grid>
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Id"/>
                    <GridViewColumn Header="Name"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
