            <ColumnDefinition Width="138*"/>
            <ColumnDefinition Width="80"/>
            <ColumnDefinition Width="80"/>
            <ColumnDefinition Width="211*"/>
        </Grid.ColumnDefinitions>

    </Grid>
</Window>
